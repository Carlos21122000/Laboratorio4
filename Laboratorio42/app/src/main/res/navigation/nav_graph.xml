<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentA">

    <fragment
        android:id="@+id/fragmentA"
        android:name="com.example.laboratorio4.FragmentA"
        android:label="fragment_a"
        tools:layout="@layout/fragment_a" >
        <action
            android:id="@+id/action_fragmentA_to_fragmentB"
            app:destination="@id/fragmentB" />
        <action
            android:id="@+id/action_fragmentA_to_fragmentC"
            app:destination="@id/fragmentC" />
        <action
            android:id="@+id/action_fragmentA_to_fragmentD"
            app:destination="@id/fragmentD" />
        <action
            android:id="@+id/action_fragmentA_to_biografia"
            app:destination="@id/biografia" />
    </fragment>
    <fragment
        android:id="@+id/fragmentB"
        android:name="com.example.laboratorio4.FragmentB"
        android:label="fragment_b"
        tools:layout="@layout/fragment_b" >
        <action
            android:id="@+id/action_fragmentB_to_fragmentInfo"
            app:destination="@id/fragmentInfo" />
    </fragment>
    <fragment
        android:id="@+id/fragmentC"
        android:name="com.example.laboratorio4.FragmentC"
        android:label="fragment_c"
        tools:layout="@layout/fragment_c" >
        <action
            android:id="@+id/action_fragmentC_to_fragmentInfo"
            app:destination="@id/fragmentInfo" />
    </fragment>
    <fragment
        android:id="@+id/fragmentD"
        android:name="com.example.laboratorio4.FragmentD"
        android:label="fragment_d"
        tools:layout="@layout/fragment_d" >
        <action
            android:id="@+id/action_fragmentD_to_fragmentInfo"
            app:destination="@id/fragmentInfo" />
    </fragment>
    <fragment
        android:id="@+id/fragmentInfo"
        android:name="com.example.laboratorio4.FragmentInfo"
        android:label="fragment_info"
        tools:layout="@layout/fragment_info" >
        <action
            android:id="@+id/action_fragmentInfo_to_fragmentA"
            app:destination="@id/fragmentA" />
    </fragment>
    <fragment
        android:id="@+id/biografia"
        android:name="com.example.laboratorio4.Biografia"
        android:label="fragment_biografia"
        tools:layout="@layout/fragment_biografia" >
        <action
            android:id="@+id/action_biografia_to_fragmentA"
            app:destination="@id/fragmentA" />
    </fragment>
</navigation>